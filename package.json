  "author": "Damian Mee <mee.damian@gmail.com> (https://meedamian.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meeDamian/google-search-cli/issues"
  },
  "homepage": "https://github.com/meeDamian/google-search-cli#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "xo": "^0.12.1"
